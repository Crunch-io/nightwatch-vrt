{
	"arrowParens": "always",
	"bracketSpacing": true,
	"quoteProps": "preserve",
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": true,
	"xmlWhitespaceSensitivity": "strict",
	"overrides": [
		{
			"files": "*.json",
			"options": { "parser": "json" }
		}
	]
}
